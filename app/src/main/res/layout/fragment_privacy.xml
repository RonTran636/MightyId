<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/holder_margin">
        <View
            android:id="@+id/view3"
            android:layout_width="140dp"
            android:layout_height="5dp"
            android:layout_marginTop="4dp"
            android:background="@drawable/border_login_button"
            android:backgroundTint="@color/primary_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/materialTextView13"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/default_margin"
            android:fontFamily="@font/roboto_regular"
            android:text="@string/switch_receive_call"
            android:textSize="@dimen/text_medium_size"
            android:layout_marginTop="@dimen/default_margin"
            app:layout_constraintEnd_toStartOf="@+id/switch_receive_call"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view3" />

        <com.google.android.material.switchmaterial.SwitchMaterial
            android:id="@+id/switch_receive_call"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/default_margin"
            app:layout_constraintBottom_toBottomOf="@+id/materialTextView13"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/materialTextView13"
            app:thumbTint="@color/primary_color"
            app:trackTint="@color/primary_color_35_percent" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/materialTextView12"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/default_margin"
            android:fontFamily="@font/roboto_regular"
            android:textSize="@dimen/text_medium_size"
            android:text="@string/receive_message_from_strangers"
            app:layout_constraintBottom_toBottomOf="@+id/switch_receive_message"
            app:layout_constraintEnd_toStartOf="@+id/switch_receive_message"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/switch_receive_call" />

        <com.google.android.material.switchmaterial.SwitchMaterial
            android:id="@+id/switch_receive_message"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/default_margin"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/switch_receive_call"
            app:thumbTint="@color/primary_color"
            app:trackTint="@color/primary_color_35_percent" />

        <com.google.android.material.switchmaterial.SwitchMaterial
            android:id="@+id/switch_receive_invite_topic"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/default_margin"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/switch_receive_message"
            app:thumbTint="@color/primary_color"
            app:trackTint="@color/primary_color_35_percent" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/materialTextView10"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/default_margin"
            android:fontFamily="@font/roboto_regular"
            android:textSize="@dimen/text_medium_size"
            android:text="@string/allow_strangers_add_your_to_topic"
            app:layout_constraintBottom_toBottomOf="@+id/switch_receive_invite_topic"
            app:layout_constraintEnd_toStartOf="@+id/switch_receive_invite_topic"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/switch_receive_message" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>